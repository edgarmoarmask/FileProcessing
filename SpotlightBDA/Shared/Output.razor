
<MudTabs Elevation="2" Rounded="true" ApplyEffectsToContainer="true" PanelClass="pa-6">
            
    @foreach (ResultStats res in ResultStatsList)
    {
                
        <MudTabPanel Text="@res.Filename" >
            @foreach(string message in res.Messages)
            {
                <MudText>@message</MudText><br/>
            }
        </MudTabPanel>
    }
</MudTabs>
   
@code {
    [Parameter] 
    public List<ResultStats> ResultStatsList { get; set; } = new();
    
}